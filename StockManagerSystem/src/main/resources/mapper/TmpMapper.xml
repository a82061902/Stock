<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.csw.mapper.TmpMapper">
    <insert id="addTmp" parameterType="com.csw.entity.Tmp">
        INSERT INTO tmp(goodsId, count, supplierName) values (#{goodsId}, #{count}, #{supplierName})
    </insert>

    <delete id="delAllTmp">
        DELETE FROM tmp
    </delete>
</mapper>